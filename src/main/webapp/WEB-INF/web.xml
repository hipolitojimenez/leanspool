<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="leanspool" version="3.0">
  <display-name>leanspool</display-name>
  <welcome-file-list>
    <welcome-file>jobs-management.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>GetPrinters</servlet-name>
  	<servlet-class>com.nioos.leanspool.servlets.GetPrintersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetPrinters</servlet-name>
  	<url-pattern>/getPrinters.json</url-pattern>
  </servlet-mapping>
</web-app>